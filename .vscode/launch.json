    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Ingest Test",
            "type": "debugpy",
            "request": "launch",
            "program": "rag_ingest2.py",
            "args": ["--library-path", "D:/RAGTest"],
            "console": "integratedTerminal"
        },
        {
            "name": "Ingest Full",
            "type": "debugpy",
            "request": "launch",
            "program": "rag_ingest.py",
            "args": ["--library-path", "D:/TivoRAGSub"],
            "console": "integratedTerminal"
        },
               {
            "name": "Query Full",
            "type": "debugpy",
            "request": "launch",
            "program": "rag_query.py",
            "args": ["--library-path", "D:/TivoRAGSub", "--top-k", "20"],
            "console": "integratedTerminal"
        },
    ]
}